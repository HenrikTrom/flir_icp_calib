{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "timestamp": {"type": "string"},
      "main_cam_serial": {"type": "string"},
      "CAMERAS": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "SerialNumber": { "type": "string" },
            "ID": { "type": "integer" },
            "Intrinsic": {
              "type": "array",
              "items": { "type": "number" },
              "minItems": 9,
              "maxItems": 9
            },
            "Extrinsic": {
              "type": "array",
              "items": { "type": "number" },
              "minItems": 16,
              "maxItems": 16
            },
            "Distortion": {
                "type": "array",
                "items": { "type": "number" },
                "minItems": 5,
                "maxItems": 5
            }
          },
          "required": ["SerialNumber", "Intrinsic", "Extrinsic"]
        }
      }
    },
    "required": ["timestamp", "main_cam_serial", "CAMERAS"]
  }
  